# ==========================================
# GTA RP Dashboard - Environment Variables
# ==========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Database (Vercel Postgres / Prisma)
PRISMA_DATABASE_URL="postgresql://user:password@host:5432/database"
POSTGRES_URL="postgresql://user:password@host:5432/database"

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=generate_with_openssl_rand_base64_32

# Discord OAuth2 Configuration
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# Discord Server Configuration
DISCORD_GUILD_ID=your_guild_id
DISCORD_ROLE_ADMIN_ID=your_admin_role_id
DISCORD_ROLE_EVALUATOR_ID=your_evaluator_role_id
DISCORD_ROLE_PLAYER_ID=your_player_role_id

# Discord Bot (optional - for notifications)
DISCORD_BOT_TOKEN=your_bot_token
DISCORD_ADMIN_CHANNEL_ID=your_channel_id

# Discord Webhook (optional - for fallback notifications)
DISCORD_WEBHOOK_URL=your_webhook_url
